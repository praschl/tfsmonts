{
  "name": "tfsmonts",
  "version": "0.0.1",
  "description": "A monitor for tfs builds and releases",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "lint": "tslint --project .",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.github.praschl.tfsmonts",
    "productName": "TFS Monitor",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "win": {
      "target": "nsis",
      "asar": true
    },
    "buildVersion": "1.0.0"
  },
  "author": "Michael Praschl",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "axios": "^0.18.0",
    "humanize-duration": "^3.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@types/electron-settings": "^3.1.1",
    "@types/mocha": "^5.2.6",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "acorn": "^6.1.1",
    "ajv": "^6.10.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-settings": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "humanize-duration-ts": "^2.0.0",
    "lodash": "^4.17.11",
    "mocha": "^6.1.2",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.15.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/praschl/tfsmonts"
  },
  "keywords": [
    "tfs",
    "team foundation server",
    "build",
    "builds",
    "release",
    "releases",
    "monitor"
  ],
  "bugs": {
    "url": "https://github.com/praschl/tfsmonts/issues"
  },
  "homepage": "https://github.com/praschl/tfsmonts#readme"
}
